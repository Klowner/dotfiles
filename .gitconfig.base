[core]
	editor = vim

[gist]
	private = yes

[github]
	user = klowner

[color]
	diff = auto
	status = auto
	branch = auto

[log]
	decorate = short

[push]
	default = simple

[alias]
	branchlog = !git log ${1+}${1-master}..HEAD --reverse --date=short --format=\"git:%h %ad %s\"
