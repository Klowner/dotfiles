*.foreground: white
*.background: black

URxvt*scrollBar: false
URxvt*depth: 32
URxvt*termName: xterm-256color
URxvt*.transparent: true
URxvt*.shading: 15
URxvt*letterSpace: 0
URxvt*font: xft:Envy Code R for Powerline:style=Regular:size=10
URxvt*boldFont: xft:Envy Code R:style=Bold:size=10
!URxvt*italicFont: xft:Envy Code R for Powerline:italic:size=10
!URxvt*font: xft:Monaco:regular:pixelsize=12
!URxvt*boldFont: xft:Monaco:bold:pixelsize=12:antialias=true
!URxvt*font: xft:Terminus:style=regular:size=9:antialias=false
!URxvt*font: xft:Inconsolata:style=Regular:size=9

urxvt.perl-ext-common: default,matcher
urxvt.url-launcher: /usr/bin/google-chrome
urxvt.matcher.button: 1
URxvt.internalBorder: 4
URxvt.pointerBlank: true

Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

!*background: #101010
*cursorColor: #d0d0d0
*color0: #101010
*color1: #960050
*color2: #66aa11
*color3: #c47f2c
*color4: #30309b
*color5: #7e40a5
*color6: #3579a8
*color7: #9999aa
*color8: #303030
*color9: #ff0090
*color10: #80ff00
*color11: #ffba68
*color12: #5f5fee
*color13: #bb88dd
*color14: #4eb4fa
*color15: #d0d0d0
