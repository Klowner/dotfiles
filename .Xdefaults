*.foreground: white
*.background: black

urxvt*scrollBar: false
urxvt*depth: 32
urxvt*termName: xterm-256color
urxvt*.transparent: true
urxvt*.shading: 15

!XTerm*termName: xterm-256color
!URxvt*termName: rxvt-unicode

URxvt*font: xft:Envy Code R for Powerline:size=10:antialias=false
URxvt*boldFont: xft:Envy Code R for Powerline:bold:size=10
URxvt*italicFont: xft:Envy Code R for Powerline:italic:size=10
!URxvt*font: xft:Monaco:style=Regular:size=9:antialias=true
!URxvt*font: xft:Terminus:style=regular:size=9:antialias=false
!URxvt*font: xft:Inconsolata:style=Regular:size=9

urxvt.perl-ext-common: default,matcher
urxvt.url-launcher: /usr/bin/google-chrome
urxvt.matcher.button: 1

!*background: #101010
*cursorColor: #d0d0d0
*color0: #101010
*color1: #960050
*color2: #66aa11
*color3: #c47f2c
*color4: #30309b
*color5: #7e40a5
*color6: #3579a8
*color7: #9999aa
*color8: #303030
*color9: #ff0090
*color10: #80ff00
*color11: #ffba68
*color12: #5f5fee
*color13: #bb88dd
*color14: #4eb4fa
*color15: #d0d0d0

